{
  "name": "caredraft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coverage:open": "npm run test:coverage && open coverage/lcov-report/index.html",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:integration": "jest --testMatch='**/*.integration.test.{ts,tsx}'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:coverage": "playwright test --reporter=html,json",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "test:ci": "npm run test:coverage -- --ci --watchAll=false --silent --maxWorkers=2",
    "test:e2e:ci": "playwright test --reporter=json,html",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "test:visual:ui": "playwright test --config=playwright-visual.config.ts --ui",
    "test:visual:update": "playwright test --config=playwright-visual.config.ts --update-snapshots",
    "test:visual:ci": "playwright test --config=playwright-visual.config.ts --reporter=json,html",
    "test:performance": "playwright test e2e/visual/performance.visual.spec.ts",
    "build:analyze": "ANALYZE=true npm run build"
  },
